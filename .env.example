ENV=dev
LOG_LEVEL=INFO
DEBUG=false

# Retrieval flags
USE_SEMANTIC=true
USE_RRF=false

# Evidence thresholds
EVIDENCE_TOPK=4
EVIDENCE_THRESHOLD=0.28

# Anthropic (generation)
LLM_PROVIDER=anthropic
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-sonnet-4-20250514

# Voyage (embeddings)
EMBEDDING_PROVIDER=voyage
VOYAGE_API_KEY=
EMBEDDING_MODEL=voyage-3.5
